<%- model_class = SubjectDecisions -%>
<div class="page-header">
  <h1><%=t '.title', :default => SubjectDecisions %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:choice_value) %></dt>
      <dd><pre class="prettyprint"><%= @subject_decisions.choice_value %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              subject_decisions_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_subject_decisions_path(@subject_decisions), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              subject_decisions_path(@subject_decisions),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

