<%- model_class = SubjectDecisions -%>
<div class="page-header">
  <h1><%=t '.title', :default => SubjectDecisions %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th><%= model_class.human_attribute_name(:choice_value) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @subject_decisions.each do |subject_decisions| %>
      <tr>
        <td><%= subject_decisions.id %></td>
        <td><%= subject_decisions.choice_value %></td>
        <td><%=l subject_decisions.created_at, format: :long %></td>
        <td>
          <%= link_to subject_decisions_path(subject_decisions), :class => 'btn btn-xs', :title => "#{ t('.show', :default => t('helpers.links.show')) }" do %>
            <%= glyph 'info-sign' %>
            <%= glyph 'pencil' %>
          <%- end -%>
          <%= link_to subject_decisions_path(subject_decisions), :method => :delete, :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-xs', :title => "#{ t('.destroy', :default => t('helpers.links.destroy')) }" do %>
            <%= glyph 'remove' %>
          <%- end -%>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_subject_decisions_path,
            :class => 'btn btn-primary' %>
